
<%set cal=new cls_calendar
cal.pick(request("iCalID"))
dim bookings
set bookings=cal.bookings
set statuslist=new cls_statusList%><script type="text/javascript" src="<%=C_DIRECTORY_QUICKERSITE%>/js/sorttable.js"></script><form action="bs_ac.asp" method="post" name="bookingForm"><input type="hidden" value="bookings" name="calaction" /><table border="0" cellpadding="4" cellspacing="0" style="border:1px solid #CCC"><tr><td>Reservations for:</td><td><select onchange="javascript:document.bookingForm.submit();" name="iCalID"><%=cal.calenderlist(cal.iId)%></select></td></tr></table></form><ul><li><a href="bs_ac.asp?iCalID=<%=cal.iId%>&amp;calAction=booking">Add Reservation</a></li></ul><table class="sortable" border="0" cellpadding="4" cellspacing="0" style="width:100%"><tr><td><strong>ID</strong></td><td>&nbsp;</td><td><strong>Status</strong></td><td><strong>Start Date</strong></td><td><strong>End Date</strong></td><td><strong>Name</strong></td><td><strong>Email</strong></td><td><strong>Phone</strong></td><td><strong>Notes</strong></td><td>&nbsp;</td></tr><%for each booking in bookings%><tr><td style="border-top:1px solid #DDD" align="center"><%=booking%></td><td style="border-top:1px solid #DDD;width:20px;height:20px;" align="center" valign="middle"><%=statuslist.getImage(bookings(booking).iStatus)%></td><td style="border-top:1px solid #DDD"><%=statuslist.showSelected("single",bookings(booking).iStatus)%></td><td style="border-top:1px solid #DDD"><%=convertEuroDate(bookings(booking).dStartDate)%></td><td style="border-top:1px solid #DDD"><%=convertEuroDate(bookings(booking).dEndDate)%></td><td style="border-top:1px solid #DDD"><%=quotrep(bookings(booking).sName)%></td><td style="border-top:1px solid #DDD" align="center"><%if not isLeeg(bookings(booking).sEmail) then%><a href="mailto:<%=quotrep(bookings(booking).sEmail)%>">Email</a><%else%>---<%end if%>&nbsp;</td><td style="border-top:1px solid #DDD" align="center"><%if not isLeeg(bookings(booking).sPhone) then%><%=quotrep(bookings(booking).sPhone)%><%else%>---<%end if%>&nbsp;</td><td style="border-top:1px solid #DDD" align="center"><%if not isLeeg(bookings(booking).sNotes) then%><a rel="example1" href="bs_ac.asp?calAction=booking&amp;iCalid=<%=cal.iId%>&amp;iBookingID=<%=booking%>&amp;seeNotes=1" style="text-decoration:none">Notes</a><%else%>---<%end if%>&nbsp;</td><td style="border-top:1px solid #DDD" align="center"><a href="bs_ac.asp?iBookingID=<%=booking%>&amp;calAction=booking&amp;iCalid=<%=cal.iId%>">Modify</a></td></tr><%next%></table><p><hr /></p><p>Preview Calendar:</p><table style="width:100%"><tr><td align="center"><table><tr><td align="center"><script type="text/javascript" src="<%=C_DIRECTORY_QUICKERSITE%>/asp/bs_ac_view.asp?time=0&amp;legend=true&amp;months=12&amp;mode=js&amp;iCalID=<%=cal.iId%>"></script></td></tr></table></td></tr></table><%set bookings=nothing
set cal=nothing%>
